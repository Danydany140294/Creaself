{# templates/base.html.twig #}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}CreaSelf{% endblock %}</title>
    
    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/base.css') }}">
    {% endblock %}

   

</head>
<body>


    {% include '__home/component/navbar.html.twig' %}

    <main class="container mt-4">

        {% block body %}
        {% include '__home/main.html.twig' %}
        {% endblock %}

    </main>


    {% include '__home/component/footer.html.twig' %}

    
       
    {% block javascripts %}
    <script src="{{ asset('public\js\main.js') }}"></script>
    {% endblock %}
</body>
</html>